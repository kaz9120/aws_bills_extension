{
  "manifest_version": 2,
  "name": "AWS請求書ツール",
  "version": "1.0",
  "description": "AWS請求書を便利にする拡張機能",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}